package com.company;

import java.util.ArrayList;

public class Main {

    //8 Write a program to empty an array list.

    public static void main(String[] args) {
        ArrayList<String> colors = new ArrayList<>();
        colors.add("Yellow");
        colors.add("Black");
        colors.add("Blue");
        colors.add("Red");
        colors.add("Green");
        colors.clear();
    }
}