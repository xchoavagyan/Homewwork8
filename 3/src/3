package com.company;

import java.util.ArrayList;
import java.util.Scanner;

public class Main {

    //3 Write a program to retrieve an element (at a specified index) from a given array list

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        ArrayList<String> colors = new ArrayList<>();
        colors.add("Yellow");
        colors.add("Black");
        colors.add("Blue");
        colors.add("Red");
        colors.add("Green");
        System.out.println("Please insert a specified index of array list");
        int index = scanner.nextInt();
        System.out.println("Element of your index is : " + colors.get(index));
    }
}