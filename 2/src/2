package com.company;

import java.util.ArrayList;

public class Main {

    //2 Write a program to insert an element into the existing  array list at the first position.

    public static void main(String[] args) {
        ArrayList<String> colors = new ArrayList<>();
        colors.add("Yellow");
        colors.add("Black");
        colors.add("Blue");
        colors.add("Red");
        colors.add("Green");
        colors.add(0, "White");
    }
}