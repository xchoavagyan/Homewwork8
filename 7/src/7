package com.company;

import java.util.ArrayList;

public class Main {

    //7 Write a program to join two array lists

    public static void main(String[] args) {
        ArrayList<String> paints = new ArrayList<>();
        paints.add("Violet");
        paints.add("Orange");
        paints.add("Pink");
        ArrayList<String> colors = new ArrayList<>();
        colors.add("Yellow");
        colors.add("Black");
        colors.add("Blue");
        colors.add("Red");
        colors.add("Green");
        ArrayList<String> paintcolors = new ArrayList<>();
        paintcolors.addAll(paints);
        paintcolors.addAll(colors);
    }
}